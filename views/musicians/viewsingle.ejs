      <a href="profile/<%= req.user._id %>/edit">Edit Profile</a>
    </div>
  </div>

  <div class="bottom-container">
    <div class="row">
      <div class="col s12 m6">
        <div class="bottom-left-container">
          <h2>Bio:</h2>
          <div class="divider"></div>
          <div class="section">
          </div>
          <div class="section">
          </div>
          <div class="section">
          </div>
        </div>
      </div>
      <div class="col s12 m6">
        <div class="bottom-right-container">
          <h2>Instruments:</h2>
          <div class="divider"></div>
        </div>
      </div>
    </div>
  </div>

</div>
