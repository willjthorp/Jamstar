<div class="secondary-header">

  <p>
    <a href="javascript:history.back()">< <span class="back">Back</span></a>
  </p>

    <h3><%= title %></h3>

</div>

<div class="content-container">

  <a class="btn-floating btn-large waves-effect waves-light teal" id="back-to-top"><i class="material-icons"><i class="material-icons">keyboard_arrow_up</i>
</i></a>

<div class="row">

  <div class="col s12 l6">
      <div class="map-container">
        <div id="jam-map"></div>
      </div>
  </div>

  <div class="col s12 l6">
    <% for (i=0; i < jams.length; i++) { %>
        <div class="card horizontal venue hoverable">
          <div class="coords"><%= jams[i].venue.location.coordinates[0] %></div>
          <div class="coords"><%= jams[i].venue.location.coordinates[1] %></div>
          <div class="img-container valign-wrapper">
            <img class="circle" src="<%= jams[i].pic_path %>">
          </div>
          <div class="card-stacked">
            <div class="card-content">
              <h2><%= jams[i].name %></h2>
              <p><%= jams[i].venue.name %></p>
              <p><%= jams[i].date %> | <%= jams[i].time %></p>
              <p>Attending: <%= jams[i].attendees.length %></p>
            </div>
            <div class="card-action">
              <a href="/jams/<%= jams[i].id %>/view">View Info</a>
            </div>
          </div>
        </div>
    <% } %>
    <% if (jams.length === 0) { %>
      <div class="center-align margin-top">
        <h2>Nothing to show!</h2>
        <h2>:(</h2>
      </div>
    <% } %>
  </div>

</div>

</div>

<script src="https://code.jquery.com/jquery-3.1.1.min.js" integrity="sha256-hVVnYaiADRTO2PzUGmuLJr8BLUSjGIZsDYGmIJLv2b8=" crossorigin="anonymous"></script>
<script type="text/javascript" src="/javascripts/maps.js"></script>

<% if (title === "Jams I'm Invited To")  { %>
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCzW4MVaD-hRL-C6F_g79sxvibZz8pDLGQ&libraries=places&callback=showInvitedMarkers"></script>
<% } else if (title === "Jams I'm Attending") { %>
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCzW4MVaD-hRL-C6F_g79sxvibZz8pDLGQ&libraries=places&callback=showAttendingMarkers"></script>
<% } else if (title === "Search All Jams") { %>
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCzW4MVaD-hRL-C6F_g79sxvibZz8pDLGQ&libraries=places&callback=showAllJamMarkers"></script>
<% } else if (title === "My Jams") { %>
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCzW4MVaD-hRL-C6F_g79sxvibZz8pDLGQ&libraries=places&callback=showMyJamMarkers"></script>
<% } %>
